    "Mysql",
    "TypeORM",
    "Express",
    "Redis"
  ],
  "author": "Evidence Rex Osikhena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rexsunon/searchapi/issues"
  },
  "homepage": "https://github.com/Rexsunon/searchapi#readme",
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.5",
    "@types/node": "^13.11.1",
    "@types/request-promise": "^4.1.46",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.7",
    "pm2": "^5.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
